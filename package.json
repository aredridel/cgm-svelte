{
  "name": "cgm-svelte",
  "description": "A CGM monitor",
  "version": "0.0.1",
  "scripts": {
    "postinstall": "patch-package",
    "dev": "env-cmd sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "env-cmd cypress run",
    "cy:open": "env-cmd cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "@aredridel/cgm-plugin-dexcom-share": "^2.0.0",
    "async-main": "^1.0.2",
    "compression": "^1.7.1",
    "env-cmd": "^10.0.1",
    "express-pino-logger": "^5.0.0",
    "express-pouchdb": "^4.2.0",
    "patch-package": "^6.5.1",
    "pino": "^6.2.1",
    "pino-pretty": "^4.0.0",
    "pouchdb-adapter-http": "^7.3.1",
    "pouchdb-adapter-idb": "^7.1.1",
    "pouchdb-adapter-node-websql": "^7.0.0",
    "pouchdb-replication": "^7.1.1",
    "resolve": "^1.12.0",
    "rxdb": "^15.24.0",
    "rxjs": "^6.5.3",
    "sirv": "^0.4.0",
    "tiny-worker": "^2.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^3.29.5",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.2.2",
    "rollup-plugin-terser": "^5.3.1",
    "sapper": "^0.27.13",
    "svelte": "^3.49.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    }
  }
}
